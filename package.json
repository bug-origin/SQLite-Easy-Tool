{
  "name": "sqlite-easy-tool",
  "displayName": "SQLite Easy Tool",
  "description": "ðŸŽ¨ Visual SQLite database editor - As easy as Excel, view and edit data without writing SQL",
  "version": "0.1.3",
  "publisher": "Dev-Winston",
  "license": "MIT",
  "icon": "images/icon.jpg",
  "author": {
    "name": "bug-origin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bug-origin/SQLite-Easy-Tool"
  },
  "bugs": {
    "url": "https://github.com/bug-origin/SQLite-Easy-Tool/issues"
  },
  "homepage": "https://github.com/bug-origin/SQLite-Easy-Tool#readme",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Data Science",
    "Visualization",
    "Programming Languages"
  ],
  "keywords": [
    "sqlite",
    "database",
    "db",
    "sql",
    "editor",
    "viewer",
    "sqlite3",
    "query",
    "data",
    "table",
    "visual editor",
    "database management"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "sqliteEditor.editor",
        "displayName": "SQLite Editor",
        "selector": [
          {
            "filenamePattern": "*.sqlite"
          },
          {
            "filenamePattern": "*.sqlite3"
          },
          {
            "filenamePattern": "*.db"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build:ext",
    "build": "npm run build:ext",
    "build:ext": "esbuild src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node && cp node_modules/sql.js/dist/sql-wasm.wasm dist/",
    "watch": "npm run build:ext -- --watch",
    "package": "vsce package",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^3.2.2",
    "esbuild": "^0.19.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "sql.js": "^1.13.0"
  }
}